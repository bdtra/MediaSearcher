@model Dictionary<string, int>
@using System.Data;

<h2>@ViewBag.Title Search Results</h2>
<br />
<br />
<table>
    <thead>
        <tr>
            <th>
                Word
            </th>
            <th>
                Value
            </th>
        </tr>
    </thead>
    <tbody>
        @if (Model.Count > 0)
        {
        foreach (var pair in Model)
        {
            <tr>
                <td>@pair.Key</td>
                <td>@pair.Value</td>
            </tr>
        }
        }
        else
        {
            <p>no search here!</p>
        }
    </tbody>
</table> 